# Server Configuration
NODE_ENV=development
PORT_STATIC=3000
PORT_FRONTEND=3001
PORT_API=3003
API_URL=http://localhost:3003
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Auth0 Configuration
REACT_APP_AUTH0_DOMAIN=your_auth0_domain
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=your_auth0_audience
REACT_APP_API_URL=your_api_url
AUTH0_FALLBACK_DOMAIN=your_auth0_fallback_domain
AUTH0_FALLBACK_ISSUER_BASE_URL=your_auth0_fallback_issuer_url
AUTH0_ISSUER_BASE_URL=your_auth0_issuer_url

# AWS Configuration (for S3 uploads)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region
AWS_S3_BUCKET=your_s3_bucket_name

# Rhino Compute Configuration - REQUIRED for backend API
REACT_APP_RHINO_COMPUTE_URL=http://13.53.137.42:80  # Used by frontend
REACT_APP_RHINO_COMPUTE_KEY=your_rhino_compute_key

# THESE ARE REQUIRED for the backend API to function
RHINO_COMPUTE_ENDPOINT=http://13.53.137.42:80  # Used by backend
RHINO_COMPUTE_KEY=your_rhino_compute_key

# Logging Configuration
ENABLE_REQUEST_LOGGING=false  # Set to true for detailed request logs in production