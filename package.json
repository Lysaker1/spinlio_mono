{
  "name": "prefab-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "src/apps/*",
    "src/shared"
  ],
  "scripts": {
    "start:design": "cd src/apps/design && npm run start",
    "start:marketplace": "cd src/apps/marketplace && npm run start",
    "build:design": "cd src/apps/design && npm run build",
    "build:marketplace": "cd src/apps/marketplace && npm run build",
    "update-imports": "./scripts/update_frontend_imports.sh",
    "install:all": "npm install && cd src/apps/design && npm install && cd ../marketplace && npm install && cd ../../shared && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@emotion/styled": "^11.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.4.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": "18.20.4",
    "npm": "10.x"
  }
}
