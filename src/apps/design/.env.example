# Design App Environment Variables - Example File
# Copy this file to .env.development or .env.production and fill in the values

# API Connection
REACT_APP_API_URL=http://localhost:3001

# Authentication (Auth0)
REACT_APP_AUTH0_DOMAIN=your-auth0-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-auth0-client-id
REACT_APP_AUTH0_AUDIENCE=https://your-api-identifier
REACT_APP_AUTH0_REDIRECT_URI=http://localhost:3000/callback
REACT_APP_AUTH0_SCOPE="openid profile email"

# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-supabase-project.supabase.co
REACT_APP_SUPABASE_KEY=your-supabase-anon-key-here

# AWS S3 Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET=your-bucket-name
REACT_APP_S3_URL=https://your-bucket-name.s3.amazonaws.com

# 3D Configuration
REACT_APP_THREE_MODEL_PATH=/models
REACT_APP_SHAPEDIVER_TICKET=your-shapediver-ticket-here

# Feature Flags
REACT_APP_ENABLE_AR_PREVIEW=true
REACT_APP_ENABLE_ANALYTICS=false

# Stripe Payments
REACT_APP_STRIPE_PUBLIC_KEY=your-stripe-public-key-here

# Deployment
REACT_APP_ENVIRONMENT=development
PUBLIC_URL=/ 